{
  "entryFile": "src/main.ts",
  "noImplicitAdditionalProperties": "throw-on-extras",
  "controllerPathGlobs": [
    "src/**/*Controller.ts"
  ],
  "spec": {
    "securityDefinitions": {
      "api_key": {
        "type": "apiKey",
        "name": "Authorization",
        "in": "header"
      }
    },
    "outputDirectory": "src",
    "specVersion": 3,
    "version": "v2 node",
    "description": "API for Lyricated app."
  },
  "routes": {
    "routesDir": "src"
  }
}
